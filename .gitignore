
const students = [
  { name: "Rahul", dob: "2004-11-28" },
  { name: "Meena", dob: "2005-03-10" }
];

function checkBirthdays() {
  const today = new Date();
  const d = today.getDate();
  const m = today.getMonth() + 1;

  students.forEach(s => {
    const date = new Date(s.dob);
    if (date.getDate() === d && date.getMonth() + 1 === m) {
      showToast(`ðŸŽ‰ Today is ${s.name}'s Birthday!`);
    }
  });
}

function showToast(msg) {
  alert(msg); // Replace with toast in UI frameworks
}

checkBirthdays();
